@model SETEC.Data.Entities.HistoricoClientesContrato

@{
    ViewData["Title"] = "Delete";
}
<div class="bg-white" style="padding: 0 20px 20px 20px;">
    <h1 class="text-danger">Eliminar Registro</h1>
    <h3 class="text-danger">¿Esta seguro que desea eliminar el registo?</h3>
<div>
    <h4>Clientes Gestionados</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Fechagestion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Fechagestion)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Identidad)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Identidad)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Contrato)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Contrato)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Codigo_Gestion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Codigo_Gestion)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Desc_gestion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Desc_gestion)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Monto_promesa)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Monto_promesa)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Fecha_Promesa)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Fecha_Promesa)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver a la lista</a>
    </form>
</div>
</div>