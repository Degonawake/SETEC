@model SETEC.Data.Entities.Codgestion

@{
    ViewData["Title"] = "Create";
}

<div class="bg-white" style="padding: 20px 0 20px 20px">

    <h1 style="color:darkslateblue">Crear codigos de gestiones</h1>

<h4>Codgestion</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="idgestion" class="control-label"></label>
                <input asp-for="idgestion" class="form-control" />
                <span asp-validation-for="idgestion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DescGestion" class="control-label"></label>
                <input asp-for="DescGestion" class="form-control" />
                <span asp-validation-for="DescGestion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Crea Codigo" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-action="Indexgestioncodes">Regresar a la lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

</div>